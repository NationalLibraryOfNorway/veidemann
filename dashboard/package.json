{
  "name": "veidemann-dashboard",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "proxychains": "proxychains ng serve --configuration development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "test:chromium": "CHROME_BIN=chromium-browser ng test",
    "build:docker": "docker build --build-arg BASE_HREF=${BASE_HREF:-/} --build-arg VERSION=$(git describe --tags --always) -t ${IMAGE:-veidemann-dashboard} .",
    "extract-i18n": "ng extract-i18n",
    "proto:generate": "(cd .. && buf generate --template buf.gen.ts.yaml)"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
