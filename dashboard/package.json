    "@ngbracket/ngx-layout": "^20.0.1",
    "@types/prismjs": "^1.26.5",
    "angular-eslint": "^20.3.0",
    "angular-oauth2-oidc": "~20.0.2",
    "chroma-js": "^3.1.2",
    "cron-schedule": "^5.0.4",
